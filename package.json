{
  "name": "microfrontends-in-practice",
  "version": "1.0.0",
  "scripts": {
    "start:server": "nodemon server.js --watch",
    "start:client": "webpack-dev-server --inline --content-base ./client/src --env.target development --open --port 3001",
    "build": "npm-run-all clean:embed clean:standalone build:embed build:standalone",
    "build:embed": "webpack --env.target embed",
    "build:standalone": "webpack --env.target standalone",
    "clean:embed": "del-cli public/embed/*",
    "clean:standalone": "del-cli public/standalone/*"
  },
  "author": "Leonardo de Lima Cristofani",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "webpack": "^4.16.5",
    "webpack-merge": "^4.1.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "del-cli": "^1.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "style-loader": "^0.22.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
